{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "aps2scala",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/aps2scala",
        "args": [
          // "-Dt",
          "-S",
          "-p",
          "..:base:examples",
          "-G",
          "../examples/first-impl"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      },
      {
        "name": "apssched",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/apssched",
        "args": [
          "-DD",
          "-p",
          "base:examples",
          "../examples/fiber-cycle-basic"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }