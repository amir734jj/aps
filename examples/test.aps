  pragma test_canonical_signature(type K2, "{PHYLUM[],GEN[K2$C]}");
  pragma test_canonical_signature(type L2, "{PHYLUM[],GEN[L2]}");

  -------------------------------------------------------

  -- polymiorphic (non-generating)
  type M := EXT[A];
  type N := EXT[B];
  type O := EXT[C];
  type P := EXT[D];

  pragma test_canonical_type(type M, "M");
  pragma test_canonical_type(type N, "N");
