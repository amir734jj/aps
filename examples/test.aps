  pragma test_canonical_signature(type G2, "{PHYLUM[]}");
  pragma test_canonical_signature(type H2, "{PHYLUM[]}");

  -------------------------------------------------------

  -- polymiorphic (generating)
  type I := GEN[A];
  type J := GEN[B];
  type K := GEN[C];
  type L := GEN[D];

